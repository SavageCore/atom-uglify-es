{
  "name": "atom-uglify-es",
  "main": "./lib/atom-uglify-es",
  "version": "0.1.0",
  "description": "Uglify Javascript with uglify-es",
  "keywords": [
    "javascript",
    "minify",
    "compress",
    "uglify",
    "es6"
  ],
  "activationCommands": {
    "atom-workspace": "atom-uglify-es:uglify",
    ".tab-bar": "atom-uglify-es:uglify-tab",
    ".tree-view": "atom-uglify-es:uglify-tree"
  },
  "repository": "https://github.com/SavageCore/atom-uglify-es",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "uglify-es": "^3.2.1"
  },
  "scripts": {
    "test": "xo"
  },
  "devDependencies": {
    "chokidar": "^1.7.0",
    "xo": "*"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "jasmine"
    ],
    "globals": [
      "atom",
      "waitsForPromise",
      "document"
    ],
    "rules": {
      "max-nested-callbacks": 0
    },
    "ignores": [
      "atom/Atom.app/**",
      "atom/Atom Beta.app/**"
    ]
  }
}
